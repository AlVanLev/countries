<%= form_for(@municipality) do |form| %>
  <%=render 'shared/error_messages', object: form.object%>

  <%= form.label :name %>
  <%= form.text_field :name, class:"form-control"%>

  <%= form.label :state_id, "Select State" %>
  <%= form.collection_select(:state_id, @states, :id, :name,{promp:''}, {class: 'form-control'}) %>

  <%= form.submit class:"btn btn-info btn-sm" %>

<% end %>

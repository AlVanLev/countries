<% provide(:title,@municipality.name) %>
<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @municipality.name %>
</p>

<p>
  <strong>State:</strong>
  <%state=State.where(id:@municipality.state_id).pluck(:name) %>
  <%= state[0] %>
</p>
<br>
<%= link_to 'Edit', edit_municipality_path(@municipality), class: "btn btn-xs" %> |
<%= link_to 'Back', municipalities_path, class: "btn btn-xs" %>
